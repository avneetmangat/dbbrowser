    <cp>lib/activation.jar</cp>
    <cp>lib/mail.jar</cp>
    <cp>lib/smtp.jar</cp>
    <cp>lib/pop3.jar</cp>
    <cp>lib/mailapi.jar</cp>
    <cp>lib/imap.jar</cp>
    <cp>lib/syntax.jar</cp>
    <cp>lib/itext-1.3.jar</cp>
