TitlebarPixmapActive=activetitlebar.PNG
TitlebarPixmapInactive=inactivetitlebar.PNG
PixmapUnderTitleText=no
TitleFrameShaded=no
TitleAlignment=middle
#-----------------------------------------------------------------------------
[Window Button Layout]
ButtonA=Close
ButtonB=Off
ButtonC=Off
ButtonD=Iconify
ButtonE=Maximize
ButtonF=Off
#-----------------------------------------------------------------------------
[Sounds]
LogoutMessage=wildeep.wav
Startup=startup.wav
WindowClose=sosumi.wav
WindowDeIconify=droplet.wav
