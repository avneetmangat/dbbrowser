# KDE Config File
#-----------------------------------------------------------------------------
[General]
name=MacOS 8
author=Robert Flemming
email=flemming@themes.org
description=A full featured MacOS theme.\nBased upon original MacOS theme by Daniel Mettler.\nWith help from my resident Mac guru Peter Bukowinski.
version=1.0
#-----------------------------------------------------------------------------
[Display]
CommonDesktop=false
Wallpaper0=desktop0.jpg
WallpaperMode0=Tiled
Wallpaper1=desktop1.jpg
WallpaperMode1=Scaled
RootIconTextStyle=0
RootIconTextForeground=255,255,255
RootIconTextBackground=0,0,0
#-----------------------------------------------------------------------------
[Colors]
contrast=7
activeBackground=206,206,206
activeBlend=206,206,206
activeForeground=0,0,0
background=206,206,206
foreground=0,0,0
inactiveBackground=206,206,206
inactiveBlend=206,206,206
inactiveForeground=107,107,107
selectBackground=49,49,156
selectForeground=255,255,255
windowBackground=255,255,255
windowForeground=0,0,0
#-----------------------------------------------------------------------------
[File Manager]
htmlBackground=255,255,255
htmlForeground=0,0,0
htmlLinks=0,0,0
htmlFollowedLinks=128,128,128
#-----------------------------------------------------------------------------
[Window Border]
shapePixmapTop=wm_top.PNG
shapePixmapBottom=wm_bottom.PNG
shapePixmapLeft=wm_left.PNG
shapePixmapRight=wm_right.PNG
shapePixmapTopLeft=wm_topleft.PNG
shapePixmapTopRight=wm_topright.PNG
shapePixmapBottomLeft=wm_bottomleft.PNG
shapePixmapBottomRight=wm_bottomright.PNG
#-----------------------------------------------------------------------------
[Window Titlebar]
CloseButton=close.PNG
MaximizeButton=maximize.PNG
MaximizeDownButton=maximizedown.PNG
